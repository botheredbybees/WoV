# PostgreSQL / Supabase Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=SuperSecretLongPassword
POSTGRES_DB=wov
POSTGRES_PORT=5435 # Note: This reflects the *host* port, internal is always 5432

# JWT secrets - make sure these are long, random, and secure strings
JWT_SECRET=supersecretjwtkeyvaluechangeme
JWT_EXPIRY=3600

# PostgREST API settings
PGRST_DB_SCHEMAS=public,storage,wov # Ensure 'wov' schema is exposed
PGRST_DB_ANON_ROLE=anon            # usually 'anon' or equivalent

# Supabase Auth/Storage keys (if you include those services)
SITE_URL=http://localhost:3003
ANON_KEY=supersecretanonkeychangeme
SERVICE_ROLE_KEY=supersecretservicerolekeychangeme
DASHBOARD_PASSWORD=supersecretdashboardpasswordchangeme
DASHBOARD_USERNAME=admin
SUPABASE_PUBLIC_URL=http://localhost:8000
NEXT_PUBLIC_SUPABASE_ANON_KEY=supersecretanonkeychangeme

# Supabase Auth settings
DISABLE_SIGNUP=false
ENABLE_EMAIL_SIGNUP=true
ENABLE_ANONYMOUS_USERS=false
ENABLE_EMAIL_AUTOCONFIRM=false
ENABLE_PHONE_SIGNUP=false
ENABLE_PHONE_AUTOCONFIRM=false

# Supabase SMTP settings (for email sending)
SMTP_ADMIN_EMAIL=admin@example.com
SMTP_HOST=localhost
SMTP_PORT=54324
SMTP_USER=
SMTP_PASS=
SMTP_SENDER_NAME=WoV

# Supabase Mailer URL paths
MAILER_URLPATHS_INVITE=/auth/v1/verify
MAILER_URLPATHS_CONFIRMATION=/auth/v1/verify
MAILER_URLPATHS_RECOVERY=/auth/v1/verify
MAILER_URLPATHS_EMAIL_CHANGE=/auth/v1/verify

# Other secrets
PGRST_JWT_SECRET=supersecretjwtkeyvaluechangeme
SECRET_KEY_BASE=supersecretkeybasechangeme
FASTAPI_API_KEY=superfastapikeychangeme
SUPABASE_DB_URL=postgresql://postgres:SuperSecretLongPassword@db:5432/wov


# Optional: InfluxDB environment variables (if you run influxdb service)
INFLUXDB_ADMIN_USER=admin
INFLUXDB_ADMIN_PASSWORD=adminpassword
INFLUXDB_ORG=nuyina
INFLUXDB_BUCKET=underway

