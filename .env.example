# PostgreSQL / Supabase Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=SuperSecretLongPassword
POSTGRES_DB=wov
POSTGRES_PORT=5435 # Note: This reflects the *host* port, internal is always 5432

# JWT secrets - make sure these are long, random, and secure strings
JWT_SECRET=supersecretjwtkeyvaluechangeme
JWT_EXPIRY=3600

# PostgREST API settings
PGRST_DB_SCHEMAS=public,storage,wov # Ensure 'wov' schema is exposed
PGRST_DB_ANON_ROLE=anon            # usually 'anon' or equivalent

# Supabase Auth/Storage keys (if you include those services)
SITE_URL=http://localhost:3003
ANON_KEY=supersecretanonkeychangeme
SERVICE_ROLE_KEY=supersecretservicerolekeychangeme
DASHBOARD_PASSWORD=supersecretdashboardpasswordchangeme
DASHBOARD_USERNAME=admin
SUPABASE_PUBLIC_URL=http://localhost:8000

# Optional: InfluxDB environment variables (if you run influxdb service)
INFLUXDB_ADMIN_USER=admin
INFLUXDB_ADMIN_PASSWORD=adminpassword
INFLUXDB_ORG=nuyina
INFLUXDB_BUCKET=underway

